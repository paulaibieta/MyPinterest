<%= form_for @pin, html: { multipart: true } do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title, placeholder: @pin.title %>
    <%= f.label :description %>
    <%= f.text_field :description, placeholder: @pin.title %>
    <%= f.file_field :image %>
    <%= f.submit 'submit corrections' %>
  <% end %>